{
  "name": "domasqljumper",
  "displayName": "Doma SQL Jumper",
  "description": "You can open the associated SQL file from Doma's Dao interface. You can also do the opposite.",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "icon": "domasqljumper_logo.png",
  "contributes": {
    "commands": [
      {
        "command": "domasqljumper.jumpSqlOrDao",
        "title": "Doma SQL Jumper: Jump SQL or Dao"
      }
    ],
    "keybindings": [
      {
        "command": "domasqljumper.jumpSqlOrDao",
        "key": "ctrl+alt+0",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "domasqljumper.jumpSqlOrDao",
          "group": "domasqljumper@1"
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daisuke-yamanaka/domasqljumper"
  },
  "publisher": "daisuke-yamanaka",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4"
  }
}
